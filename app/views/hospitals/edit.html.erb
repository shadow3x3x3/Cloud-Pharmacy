

<%= form_for @hospital, :url => hospital_path(@hospital), :method => :patch,
                        :html => { :class => 'form-horizontal' } do |f| %>
  <h1>編輯醫院資料</h1>

  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= f.submit "Update", class: "btn btn-default btn-primary" %>

<% end %>