<h1>管理會員</h1>
<ul>
<table class="table table-bordered ">
  <tr>
    <th>會員姓名</th>
    <th>身分證字號</th>
    <th>Email</th>
    <th>電話</th>
  </tr>
  <% @members.each do |member| %>
  <tr>
      <td><%= member.name %></td>
      <td><%= member.memberIdNumber %></td>
      <td><%= member.email %></td>
      <td><%= member.phone %></td>
    <td>
      <%= link_to '編輯', edit_member_path(member) %>
    </td>
    <td>
      <%= link_to '刪除', member_path(member), :method => :delete %>
    </td>
  </tr>
  <% end %>

</table>
</ul>

<%= link_to '新增會員', new_member_path %>
<br \>
<%= paginate @members %>