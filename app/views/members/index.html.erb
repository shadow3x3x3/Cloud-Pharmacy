<h1>管理會員</h1>
<br \>
<ul>
<table class="table table-bordered table-striped table-hover sortable">
  <tr>
    <th>會員姓名</th>
    <th>身分證字號</th>
    <th>Email</th>
    <th>電話</th>
  </tr>
  <% @members.each do |member| %>
  <tr>
    <td><%= member.name %></td>
    <td><%= member.memberIdNumber %></td>
    <td><%= member.email %></td>
    <td><%= member.phone %></td>
  </tr>
  <% end %>

</table>
</ul>

<br \>
<%= paginate @members %>
