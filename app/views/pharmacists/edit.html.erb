<%= form_for @pharmacists, :url => pharmacists_path(@pharmacists), :method => :patch,
                           :html => { :class => 'form-horizontal' } do |f| %>
  <h1>編輯藥師資料</h1>

  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= f.submit "Update", class: "btn btn-default btn-primary" %>

<% end %>